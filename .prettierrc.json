{
  "endOfLine": "lf",
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "react",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/types/",
    "^@/utils/",
    "^@/store/",
    "^@/hooks/",
    "^@/contexts/",
    "^@/assets/",
    "^@/components/",
    "",
    "^[./]"
  ]
}
